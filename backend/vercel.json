{
  "version": 2,
  "builds": [{ "src": "./app.js", "use": "@vercel/node" }],
  "routes": [{ "src": "/.*", "dest": "/" }],
  "env": {
    "NODE_ENV": "production"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ],
  "rewrites": [{ "source": "/old-path", "destination": "/new-path" }]
}
